<div class="panel-body">
  <p>
    <strong>Title:</strong>
    <%= @event.title %>
  </p>

  <p>
    <strong>Start date:</strong>
    <%= @event.start_date %>
  </p>

  <p>
    <strong>Review date:</strong>
    <%= @event.review_date %>
  </p>

  <p>
    <strong>End date:</strong>
    <%= @event.end_date %>
  </p>

  <p>
    <strong>Tags:</strong>
    <%= @event.tags %>
  </p>

  <p>
    <strong>Completed:</strong>
    <%= @event.completed %>
  </p>
</div>

<%= link_to 'Edit', edit_event_path(@event) %> |
<%= link_to 'Back', events_path %>